{
  "name": "hooks",
  "scope": "umijs",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "github": {
    "enabled": false
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "alias": [
    "hooks.umijs.org"
  ]
}
